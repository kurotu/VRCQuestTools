{
  "problemMatcher": [
    {
      "owner": "csharp",
      "pattern": [
        {
          "regexp": "^\\s*(.+)\\((\\d+),(\\d+)\\):\\s+(warning|error)\\s+(\\w+):\\s+(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "code": 5,
          "message": 6
        }
      ]
    }
  ]
}
